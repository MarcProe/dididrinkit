{
  "name": "dididrinkit",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:host": "export NODE_OPTIONS=--openssl-legacy-provider; nuxt --hostname '0' --port 3000",
    "heroku:logs": "heroku logs --tail",
    "git:push-heroku-master": "git push heroku master",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider; nuxt build",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider; nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "prepare": "husky install",
    "lint": "npm run lint:js && npm run lint:style",
    "test:e2e": "nightwatch",
    "fuck_modules": "rm -rf node_modules && rm -f package-lock.json && npm i"
  },
  "engines": {
    "node": "v16.x"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/color-mode": "^2.1.1",
    "@nuxtjs/pwa": "^3.3.5",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.15.1",
    "dotenv": "^10.0.0",
    "nuxt": "^2.15.7",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/lodash-es": "^4.17.5",
    "chromedriver": "^91.0.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "nightwatch": "^1.7.6",
    "node-sass": "^6.0.1",
    "prettier": "^2.3.2",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "webpack": "^4.46.0"
  }
}
